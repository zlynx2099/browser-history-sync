{
  "name": "Browser Sync",
  "version": "1.0",
  "description": "Browser Sync is an extension to synchronize your bookmarks and profile across your browsers (including Kiwi Browser)",
  "background": {
    "scripts": ["zl-indexdb.js","chromeApi.js","pageUtil.js","background.js"],
    "persistent": true
  },
  "icons":{
    "512":"icons/moebius-star.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "minimum_chrome_version": "9",
  "permissions": ["storage","bookmarks", "tabs","history"],
  "manifest_version": 2
}
